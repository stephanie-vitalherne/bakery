<div class='ui grid contact'>
  <div class="two column row">
    <div class="column">
      <h2>Come and Visit!</h2>
      <address>
        401 Bleecker Street & W. 11th Street <br>
        New York, NY <br>
        <em>212.462.2572</em>
      </address>
    </div>
    <div class="column two">
      <h1>Events to See Us At!</h1>
      <div id="events">
      <div>
        <% @event.first(3).each do |event| %>
          <img src="<%= event['logo']['original']['url']%>" width="100%" height="300px">
          <h3><%= event['name']['text']%></h3>
        <% end %>
      </div>
    </div>
    </div>
  </div>
</div>
